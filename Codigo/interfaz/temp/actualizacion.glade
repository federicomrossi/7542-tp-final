    <child>
      <object class="GtkBox" id="box1">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkLabel" id="lblTitulo">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="halign">start</property>
            <property name="valign">end</property>
            <property name="margin_left">30</property>
            <property name="margin_right">18</property>
            <property name="margin_top">44</property>
            <property name="label" translatable="yes">Sincronizando directorio</property>
            <attributes>
              <attribute name="font-desc" value="Sans 16"/>
            </attributes>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="position">0</property>
          </packing>
        </child>
        <child>
          <object class="GtkLabel" id="lblActividad">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="halign">start</property>
            <property name="margin_left">30</property>
            <property name="margin_top">22</property>
            <property name="xalign">0.54000002145767212</property>
            <property name="yalign">0.44999998807907104</property>
            <property name="label" translatable="yes">Solicitando archivos al servidor</property>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="position">1</property>
          </packing>
        </child>
        <child>
          <object class="GtkProgressBar" id="prgbarCompletado">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="margin_left">30</property>
            <property name="margin_right">30</property>
            <property name="margin_top">2</property>
            <property name="margin_bottom">6</property>
            <property name="pulse_step">0.099999999776482579</property>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="position">2</property>
          </packing>
        </child>
        <child>
          <object class="GtkLabel" id="lblVerDetalles">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="events">GDK_POINTER_MOTION_MASK | GDK_BUTTON_MOTION_MASK | GDK_STRUCTURE_MASK</property>
            <property name="halign">start</property>
            <property name="valign">end</property>
            <property name="margin_left">30</property>
            <property name="xalign">0.54000002145767212</property>
            <property name="yalign">0.44999998807907104</property>
            <property name="label" translatable="yes">Mostrar detalles</property>
            <attributes>
              <attribute name="font-desc" value="&lt;Introducir valor&gt; 9"/>
              <attribute name="underline" value="True"/>
              <attribute name="foreground" value="#1f4f0000ffff"/>
            </attributes>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="position">3</property>
          </packing>
        </child>
